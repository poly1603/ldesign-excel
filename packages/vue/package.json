{
  "name": "@excel-renderer/vue",
  "version": "0.1.0",
  "description": "Excel渲染插件Vue适配包",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vue-tsc && vite build",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist",
    "test": "vitest"
  },
  "keywords": [
    "excel",
    "spreadsheet",
    "vue",
    "vue3",
    "component"
  ],
  "author": "Excel Renderer Team",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@excel-renderer/core": "workspace:*",
    "@vueuse/core": "^10.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.0.4",
    "vue": "^3.3.10",
    "vue-tsc": "^1.8.25"
  }
}